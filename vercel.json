{
  "version": 2,
  "builds": [
    {
      "src": "./backend",
      "use": "@vercel/node",
      "command": "npm install"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/index.js"
    }
  ],
  "build": {
    "commands": [
      "cd backend",
      "npm install"
    ]
  }
}
